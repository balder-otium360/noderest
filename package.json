    "coffee-script": "^1.8.0",
    "compression": "^1.3.0",
    "express": "^4.11.0",
    "mongoose": "^3.8.0",
    "morgan": "^1.5.0",
    "q": "^1.1.0",
    "underscore": "^1.7.0",
    "winston": "^0.8.0"
